# Backend Environment Variables
# Copy this file to .env and update with your actual values

# PORT is auto-assigned by Render in production - only set for local development
PORT=5000

# MongoDB connection string
MONGODB_URI=mongodb://127.0.0.1:27017/socialAnalyticsDB

# JWT secret key - MUST be changed in production
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# Node environment
NODE_ENV=development

# Frontend URL for CORS (only needed in production)
# FRONTEND_URL=https://YOUR_VERCEL_FRONTEND_URL
